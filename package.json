    "next": "^14.0.4",
    "pg": "^8.11.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwind-merge": "^2.2.0",
    "@revenuecat/purchases-js": "^1.0.3",
    "@types/node": "^20.19.1",
    "@types/pg": "^8.10.9",
