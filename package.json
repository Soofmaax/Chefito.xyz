{
  "name": "chefito-professional-cooking-assistant",
  "version": "1.0.0",
  "private": true,
  "description": "Professional voice-guided cooking assistant platform built for World's Largest Hackathon by Bolt",
  "author": "Salwa Essafi (@Soofmaax)",
  "keywords": [
    "cooking",
    "voice-ai",
    "recipes",
    "nextjs",
    "typescript",
    "supabase",
    "elevenlabs",
    "hackathon",
    "pwa",
    "android",
    "capacitor"
  ],
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint --fix",
    "lint:check": "next lint",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf .next out node_modules/.cache",
    "build:clean": "npm run clean && npm install && npm run build",
    "prebuild": "npm run type-check",
    "export": "next build && next export",
    "read-recipe": "ts-node src/cli/readRecipe.ts",
    "bulk-import": "ts-node src/cli/bulkImportRecipes.ts",
    "import-recipes": "ts-node src/cli/importRecipes.ts",
    "export-template": "ts-node src/cli/exportRecipesTemplate.ts",
    "build:web": "next build",
    "build:capacitor": "CAPACITOR_BUILD=true next build",
    "cap:build": "npm run build:capacitor && cap sync",
    "cap:run:android": "npm run cap:build && cap run android",
    "cap:run:ios": "npm run cap:build && cap run ios",
    "cap:build:android": "npm run cap:build && cap build android",
    "cap:build:ios": "npm run cap:build && cap build ios",
    "test": "mocha -r ts-node/register test/**/*.test.ts"
  },
  "dependencies": {
    "@capacitor/android": "^5.6.0",
    "@capacitor/app": "^5.0.6",
    "@capacitor/browser": "^5.1.0",
    "@capacitor/camera": "^5.0.7",
    "@capacitor/core": "^5.6.0",
    "@capacitor/device": "^5.0.6",
    "@capacitor/filesystem": "^5.1.4",
    "@capacitor/haptics": "^5.0.6",
    "@capacitor/ios": "^5.6.0",
    "@capacitor/keyboard": "^5.0.6",
    "@capacitor/local-notifications": "^5.0.6",
    "@capacitor/network": "^5.0.6",
    "@capacitor/push-notifications": "^5.1.0",
    "@capacitor/splash-screen": "^5.0.6",
    "@capacitor/status-bar": "^5.0.6",
    "@capacitor/toast": "^5.0.6",
    "@supabase/auth-helpers-nextjs": "^0.8.7",
    "@supabase/supabase-js": "^2.50.1",
    "autoprefixer": "^10.4.16",
    "clsx": "^2.0.0",
    "csv-parse": "^5.5.5",
    "csv-stringify": "^6.4.6",
    "lucide-react": "^0.294.0",
    "next": "^14.0.4",
    "next-pwa": "^5.6.0",
    "pg": "^8.11.3",
    "postcss": "^8.4.32",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwind-merge": "^2.2.0",
    "tailwindcss": "^3.4.0"
  },
  "devDependencies": {
    "@capacitor/cli": "^5.6.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.19.1",
    "@types/pg": "^8.10.9",
    "@types/react": "18.3.23",
    "@types/react-dom": "^18.2.18",
    "@typescript-eslint/eslint-plugin": "^8.35.0",
    "@typescript-eslint/parser": "^8.35.0",
    "eslint": "^8.56.0",
    "eslint-config-next": "^14.0.4",
    "mocha": "^10.3.0",
    "ts-node": "^10.9.2",
    "typescript": "5.8.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
