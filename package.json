{
  "name": "chefito-professional-cooking-assistant",
  "version": "1.0.0",
  "private": true,
  "description": "Professional voice-guided cooking assistant platform built for World's Largest Hackathon by Bolt",
  "author": "Salwa Essafi (@Soofmaax)",
  "keywords": [
    "cooking",
    "voice-ai",
    "recipes",
    "nextjs",
    "typescript",
    "supabase",
    "elevenlabs",
    "hackathon",
    "pwa",
    "android",
    "capacitor"
  ],
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint --fix",
    "lint:check": "next lint",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf .next out node_modules/.cache",
    "build:clean": "npm run clean && npm install && npm run build",
    "prebuild": "npm run type-check",
    "export": "next build && next export",
    "android:init": "npx cap add android",
    "android:build": "npm run build && npx cap copy android && npx cap sync android",
    "android:dev": "npm run android:build && npx cap open android",
    "android:release": "npm run build && npx cap copy android && npx cap sync android && cd android && ./gradlew assembleRelease",
    "android:bundle": "npm run build && npx cap copy android && npx cap sync android && cd android && ./gradlew bundleRelease",
    "ios:init": "npx cap add ios",
    "ios:build": "npm run build && npx cap copy ios && npx cap sync ios",
    "ios:dev": "npm run ios:build && npx cap open ios",
    "capacitor:sync": "npx cap sync",
    "capacitor:update": "npx cap update",
    "generate:favicon": "node scripts/generate_favicon.js"
  },
  "dependencies": {
    "@capacitor/android": "^5.6.0",
    "@capacitor/app": "^5.0.6",
    "@capacitor/browser": "^5.1.0",
    "@capacitor/camera": "^5.0.7",
    "@capacitor/core": "^5.6.0",
    "@capacitor/device": "^5.0.6",
    "@capacitor/filesystem": "^5.1.4",
    "@capacitor/haptics": "^5.0.6",
    "@capacitor/ios": "^5.6.0",
    "@capacitor/keyboard": "^5.0.6",
    "@capacitor/local-notifications": "^5.0.6",
    "@capacitor/network": "^5.0.6",
    "@capacitor/push-notifications": "^5.1.0",
    "@capacitor/splash-screen": "^5.0.6",
    "@capacitor/status-bar": "^5.0.6",
    "@capacitor/toast": "^5.0.6",
    "@supabase/auth-helpers-nextjs": "^0.8.7",
    "@supabase/supabase-js": "^2.50.1",
    "clsx": "^2.0.0",
    "lucide-react": "^0.294.0",
    "next": "^14.0.4",
    "next-pwa": "^5.6.0",
    "pg": "^8.11.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwind-merge": "^2.2.0"
  },
  "devDependencies": {
    "@capacitor/cli": "^5.6.0",
    "@types/node": "20.19.1",
    "@types/pg": "^8.10.9",
    "@types/react": "18.3.23",
    "@types/react-dom": "^18.2.18",
    "@typescript-eslint/eslint-plugin": "^8.35.0",
    "@typescript-eslint/parser": "^8.35.0",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.56.0",
    "eslint-config-next": "^14.0.4",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.4.0",
    "typescript": "5.8.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}